	email = glenn.lai@bosondata.com.cn
	name = laixintao
[push]
	default = simple
[core]
	editor = vim
	excludesfile = ~/.gitignore
[alias]
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	d = difftool
	pr = pullrequest
	br = branch
	ck = checkout
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
